  "pages": [
    "pages/linkbridge/login/login",
    "pages/linkbridge/register/register",
    "pages/linkbridge/dashboard/dashboard",
    "pages/linkbridge/search/search",
    "pages/linkbridge/settings/settings",
    "pages/linkbridge/archive/archive",
    "pages/linkbridge/chat/chat"
  ],
  "themeLocation": "theme.json",
  "usingComponents": {
    "t-button": "tdesign-miniprogram/button/button",
    "t-icon": "tdesign-miniprogram/icon/icon",
    "t-navbar": "tdesign-miniprogram/navbar/navbar"
  },
  "window": {
    "navigationBarTitleText": "LinkBridge",
    "navigationStyle": "custom",
    "navigationBarTextStyle": "@navTxtStyle"
