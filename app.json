{
  "pages": [
    "pages/message/index",
    "pages/nearby/index",
    "pages/my/index",
    "pages/my/info-edit/index"
  ],
  "usingComponents": {
    "t-toast": "tdesign-miniprogram/toast/toast"
  },
  "permission": {
    "scope.userLocation": {
      "desc": "用于设置我的位置并在本地信息流中展示附近点位"
    }
  },
  "requiredPrivateInfos": ["getLocation", "chooseLocation"],
  "subpackages": [
    {
      "root": "pages/chat",
      "name": "chat",
      "pages": ["index"]
    },
    {
      "root": "pages/invite",
      "name": "invite",
      "pages": ["index"]
    },
    {
      "root": "pages/login",
      "name": "login",
      "pages": ["login"]
    },
    {
      "root": "pages/call",
      "name": "call",
      "pages": ["index"]
    },
    {
      "root": "pages/linkbridge/add-friend",
      "name": "linkbridge-add-friend",
      "pages": ["add-friend"]
    },
    {
      "root": "pages/archive",
      "name": "archive",
      "pages": ["index"]
    },
    {
      "root": "pages/ai-chat",
      "name": "ai-chat",
      "pages": ["index"]
    },
    {
      "root": "pages/peer",
      "name": "peer",
      "pages": ["index"]
    },
    {
      "root": "pages/localfeed",
      "name": "localfeed",
      "pages": ["profile/index"]
    }
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "Weixin",
    "navigationBarTextStyle": "black"
  },
  "tabBar": {
    "custom": true,
    "list": [
      {
        "pagePath": "pages/message/index",
        "text": "会话"
      },
      {
        "pagePath": "pages/nearby/index",
        "text": "本地"
      },
      {
        "pagePath": "pages/my/index",
        "text": "我的"
      }
    ]
  },
  "resolveAlias": {
    "~/*": "/*"
  },
  "sitemapLocation": "sitemap.json"
}
